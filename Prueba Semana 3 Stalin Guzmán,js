//Tienes un array de nÃºmeros y debes retornar true si dentro de los nÃºmeros de ese array al menos uno es un nÃºmero par
let var1 = 1;
let var2 = 20;
let i=var1;

for(i>var1; i<=var2; i++){
    
    if(i%2==0){
        console.log(i);
    }
}

//Realizar con un bucle for la tabla de multiplicar del 4
var table = prompt("Que tabla quieres imprimir:");

for (var i = 1; i < 11; i++){
    document.write(table + "x" + i + "=" + (table * i) + "<br>");
}